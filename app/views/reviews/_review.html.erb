<p><%= review.rating %></p>
<p><%= review.comment %></p>

<% if review.user_id == current_user.id %>
    <%= link_to "Edit", edit_organization_review_path(review.organization, review) %>
    <%= link_to "Delete", organization_review_path(review.organization, review), method: :delete, data: { confirm: "Are you sure?" } %>
<% end %>