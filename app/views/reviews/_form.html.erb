<%= simple_form_for([@organization, @organization.reviews.build]) do |f| %>
<center>
  <div class="field">
    <%= f.input :rating %>
  </div>
  <div class="field">
    <%= f.input :comment %>
  </div>
</center>
  <div class="actions">
    <%= f.submit :class=> "btn btn-primary",style: "background-color: #91D6F1;"%>
  </div>
<% end %>